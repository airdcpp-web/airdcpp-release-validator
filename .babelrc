{
  "presets": [ 
    ["@babel/env", {
      "targets": {
        "node": "0.10"
      },
      "useBuiltIns": "usage"
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-async-to-generator",
    "@babel/plugin-transform-object-assign",
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ],
  "sourceType": "unambiguous"
}