{
  "presets": [ 
    ["@babel/env", {
      "targets": {
        "node": "4"
      },
      "useBuiltIns": "usage",
      "corejs": 3
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-async-to-generator",
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ],
  "sourceType": "unambiguous"
}